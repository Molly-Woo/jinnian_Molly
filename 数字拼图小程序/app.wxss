/**app.wxss**/

page {
    background: #fff;
    color: #121212;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    background-size : 100% 100%;
}

.f_center {
    display: flex;
    align-items: center;
    justify-content: center;
}

button {
    background: initial;
}

button:focus {
    outline: 0;
}

button::after {
    border: none;
}

@font-face {
  font-family: 'iconfont';  /* project id 2116818 */
  src: url('//at.alicdn.com/t/font_2116818_wn374sarhn9.eot');
  src: url('//at.alicdn.com/t/font_2116818_wn374sarhn9.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_2116818_wn374sarhn9.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_2116818_wn374sarhn9.woff') format('woff'),
  url('//at.alicdn.com/t/font_2116818_wn374sarhn9.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_2116818_wn374sarhn9.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

